{
    "env":   {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
      "no-console": "off",
      "no-plusplus": "off",
      "consistent-return": "off",
      "prefer-destructuring": "off",
      "import/no-dynamic-require": "off",
      "no-empty": "off"
    }
}
